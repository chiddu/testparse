				</a><script type="text/javascript"> 
			
				
				$("#reg-fbc-login-button").die();
				$("#reg-fbc-login-button").live("click",function(){
				
				
							

			document.domain = "shaadi.com";

			var width = 500;
			var height = 500;
			
			var left = ((window.innerWidth / 2) - (width / 2)) + window.screenLeft;
			var top = ((window.innerHeight / 2) - (height / 2)) + window.screenTop;
			
			var goUrl = "";
			
				
			winUrl = "http://www.shaadi.com/fb-login?community=http://www.shaadi.com&req=signin&btype=reg_facebook_login"+goUrl;
				
			winObj = window.open(winUrl, "fbwindow", "height="+width+",width="+height+",top="+top+",left="+left);;
			});
            
			
			
			function processFacebookLogin_reg_facebook_login(e) {
				if(e.data != "error" && e.data != "missing_param" && e.origin == "http://www.shaadi.com"){
					accessToken = e.data;
					
				if("reg_facebook_login" == "reg_facebook_signup" || "reg_facebook_login" == "reg_facebook_login"){
					$.ajax({
						async: false,
						url: "/registration/ajax/social-media-data-process",
						type: "POST",
						dataType: "json",
						data: 
						{
							"medium" :"facebook",
							"accessToken" : accessToken,
							"socialClickSource" : "reg_facebook_login"
						},
						success: function(data)
						{
							data.response_url = "http://www.shaadi.com/registration/user/index/rm/fb";
							socialMediaReg(data);
						}
					});
				}
				else{
					setTimeout(function(){
						if("" == "stop-page-mobile" || "" == "stop-page")
						{
							 var location = "http://www.shaadi.com/registration/user/index/rm/fb&accessToken=" + accessToken + "&";
							 this.location = location;
						}
						else 
						{
							var location = "http://www.shaadi.com/registration/user/index/rm/fb?accessToken=" + accessToken + "&";
							this.location = location
						}
					},500);
				}
				}
			};
		</script>
								<div class="clearfix"></div>
							</div>
							<script>
								function socialMediaReg(data)
								{	 							 
									var redirectUrl = "http://www.shaadi.com/registration/user/login-submit/msg/"+data.rm;

									if(data != undefined && data != "")
									{
										if(data.status == "reg1_5")
										{
											redirectUrl = data.response_url;
										}
										else if(data.status == "auto_login")
										{
											redirectUrl = "http://www.shaadi.com/registration/user/login-submit/enc_log/"+data.el+"/enc_pass/"+data.ep;
										}
										
										if(data.uid != undefined && data.uid != "")
										{
											redirectUrl = redirectUrl + "/uid/" +data.uid;
										}
									}

									setTimeout(function(){
										this.location = redirectUrl;
									},500);
								}
							</script>
						</div>
					</div>
